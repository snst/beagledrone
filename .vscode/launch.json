    "version": "0.2.0",
    "configurations": [
         {
            "type": "gdb",
            "request": "attach",
            "name": "arducopter gdbserver",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "executable": "${workspaceRoot}/ardupilot/build/blue/bin/arducopter",
            "target": "192.168.0.195:9999",
            "args": ["-C", "udp:192.168.0.173:14550"],
            "remote": true,
            "printCalls": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        }
    ]
}
